class Animal
{
	String name;
	int age;
	String species;
	Animal(String name,int age,String species)
	{
		this.name=name;
		this.age=age;
		this.species=species;
	}
	public void printAnimal()
	{
		System.out.println("\n\tANIMALS");
		System.out.println("Name	:"+name);
		System.out.println("Age	:"+age);
		System.out.println("Species	:"+species);
	}
}
class Dog extends Animal
{
	String breed;
	String color;
	Dog(String name,int age,String species,String breed,String color)
	{
		super(name,age,species);
		this.breed=breed;
		this.color=color;
	}
	public void printDog()
	{
		System.out.println("\n\tCAT");
		System.out.println("Name	:"+name);
		System.out.println("Age	:"+age);
		System.out.println("Species	:"+species);
		System.out.println("breed	:"+breed);
		System.out.println("Color	:"+color);
	}
}

public class P1{
    public static void main(String[] args){
		Animal animal=new Animal("LION",10,"WILD");
		animal.printAnimal();
		Dog dog=new Dog("ROVAN",8,"DOMESTIC","CHIPPI PARAI","BLACK");
		dog.printDog();
	}
}

